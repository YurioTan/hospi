<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="partner_class_view_tree" model="ir.ui.view">
        <field name="name">partner.class.view.tree</field>
        <field name="model">partner.class</field>
        <field name="arch" type="xml">

            <tree editable="top" create="1" delete="1" multi_edit="1">
                <field name="name" required="1" />
                <field name="active" invisible="1" />
            </tree>

        </field>
    </record>

    <record id="partner_class_configuration_action" model="ir.actions.act_window">
        <field name="name">Partner Class</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">partner.class</field>
        <field name="view_mode">tree</field>
        <field name="domain">[]</field>
        <field name="context">{}</field>
        <field name="view_id" ref="partner_class_view_tree" />
        <field name="help" type="html">
            <p />
        </field>
    </record>

